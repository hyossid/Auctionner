{
  "name": "Auctionner",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/hyossid/Auctionner.git",
  "author": "hyossid <huvsid@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:all": "REPORT_GAS=true hardhat test",
    "test:auctioneer": "REPORT_GAS=true hardhat test test/Auctioneer.test.ts",
    "build": "hardhat compile",
    "lint": "prettier --check '{contracts,scripts,test}/**/*.{ts,json,js,sol}'"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "concurrently": "^7.5.0",
    "eth-crypto": "^2.4.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.2",
    "hardhat-erc1820": "^0.1.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-solidity": "^1.0.0",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@balancer-labs/v2-solidity-utils": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.8.0",
    "@typechain/ethers-v5": "^10.1.1",
    "@types/bytes": "^3.1.1",
    "ganache-cli": "^6.12.2",
    "hardhat-typechain": "^0.3.5",
    "typechain": "^8.1.1"
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx,sol}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "printWidth": 80,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-solidity"
    ],
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "printWidth": 80,
          "tabWidth": 2,
          "useTabs": false,
          "singleQuote": false,
          "bracketSpacing": false
        }
      }
    ]
  }
}
